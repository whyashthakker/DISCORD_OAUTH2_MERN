<div class="box">
    <article class="media">
      <div class="media-left">
        <figure class="image is-64x64">
          <img src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>.png" alt="Image">
        </figure>
      </div>
      <div class="media-content">
        <div class="content">
          <p>
            <strong><%= guild.name %></strong> <small><%= guild.id %></small> <small><%= guild.owner %></small>
            <% guild.features.forEach(feature => { %>
                <li><%= feature %></li>
            <% }) %> 
          </p>
        </div>
      </div>
    </article>
  </div>